pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello World'
bat 'javac jenkinsfile.java'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello World'
bat 'java jenkinsfile.java'

            }
        }
    }
}
